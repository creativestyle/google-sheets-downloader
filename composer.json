{
    "name": "creativestyle/google-sheets-downloader",
    "description": "Library for downloading Google Sheets as CSV files",
    "type": "library",
    "authors": [
        {
            "name": "Marek Zabrowarny",
            "email": "marek.zabrowarny@creativestyle.pl"
        }
    ],
    "require": {
        "symfony/console": "^3.2.3",
        "symfony/dependency-injection": "^3.2.3",
        "symfony/config": "~3.0",
        "google/apiclient": "^2.0"
    },
    "require-dev": {
        "phploc/phploc": "^3.0",
        "phpmd/phpmd": "^2.6",
        "squizlabs/php_codesniffer": "^2.8",
        "phpunit/phpunit": "^6.0",
        "mikey179/vfsStream": "^1.6.4"
    },
    "autoload": {
        "psr-4": {
            "Creativestyle\\GoogleSheetsDownloader\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Creativestyle\\GoogleSheetsDownloader\\Test\\": "tests"
        }
    },
    "bin": ["bin/google-sheets-downloader"]
}
